<launch>

<include file="$(find control)/launch/joy_control.launch"/>

 <node name="servo_handler" type="servo_handler" pkg="servo_handler" output="screen" />

 <node name="valve_handler" type="valve_handler" pkg="solenoid_valve_handler" output="screen" />

<node
 pkg="rosserial_python"
 type="serial_node.py"
 name="serial_node_sub"
 output="screen"
>

<param name="port" value="/dev/ttyACM0"/>
<param name="baud" value="57600"/>
</node>


</launch>
